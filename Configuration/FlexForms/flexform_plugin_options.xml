<T3DataStructure>
  <sheets>
    <sDEF>
      <ROOT>
        <TCEforms>
          <sheetTitle>Options</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <settings.selectedWishlist>
            <TCEforms>
              <label>Selectbox your Wishlist:</label>
              <config>
                <type>select</type>
                <items type="array" />
                <foreign_table>tx_cbwishlist_domain_model_wishlist</foreign_table>
              </config>
            </TCEforms>
          </settings.selectedWishlist>
          <settings.giftsPerPage>
            <TCEforms>
              <label>Max. number of gifts to display per page:</label>
              <config>
                <type>input</type>
                <size>2</size>
                <eval>int</eval>
                <default>10</default>
              </config>
            </TCEforms>
          </settings.giftsPerPage>
          <settings.selfReservation>
            <TCEforms>
              <label>Self Reservation</label>
              <config>
                <type>check</type>
              </config>
            </TCEforms>
          </settings.selfReservation>
          <settings.selectedColumns>
            <TCEforms>
              <label>Select your Colums to show on singel gift:</label>
              <config>
                <type>select</type>
                <multiple>1</multiple>
                <size>10</size>
                <minitems>1</minitems>
                <maxitems>20</maxitems>
                <autosizemax>10</autosizemax>
                <items type="array">
                  <numIndex index="0" type="array">
                    <numIndex index="0">Name</numIndex>
                    <numIndex index="1">name</numIndex>
                  </numIndex>
                  <numIndex index="1" type="array">
                    <numIndex index="0">Price</numIndex>
                    <numIndex index="1">price</numIndex>
                  </numIndex>
                  <numIndex index="2" type="array">
                    <numIndex index="0">Description</numIndex>
                    <numIndex index="1">description</numIndex>
                  </numIndex>
                  <numIndex index="3" type="array">
                    <numIndex index="0">Image</numIndex>
                    <numIndex index="1">image</numIndex>
                  </numIndex>
                  <numIndex index="4" type="array">
                    <numIndex index="0">Reserved By</numIndex>
                    <numIndex index="1">reservedby</numIndex>
                    <foreign_table>tx_cbwishlist_domain_model_giftreserver</foreign_table>
                  </numIndex>
                  <numIndex index="5" type="array">
                    <numIndex index="0">Reservdate</numIndex>
                    <numIndex index="1">reservdate</numIndex>
                  </numIndex>
                  <numIndex index="6" type="array">
                    <numIndex index="0">Reservcomment</numIndex>
                    <numIndex index="1">reservecomment</numIndex>
                  </numIndex>
                </items>
              </config>
            </TCEforms>
          </settings.selectedColumns>
        </el>
      </ROOT>
    </sDEF>
  </sheets>
</T3DataStructure>
